{
    "paths": {
        "baseDir": "addons/plugify",
        "extensionsDir": "extensions",
        "configsDir": "configs",
        "dataDir": "data",
        "logsDir": "logs",
        "cacheDir": "cache"
    },
    "loading": {
        "preferOwnSymbols": ${PLUGIFY_PREFER_OWN_SYMBOLS},
        "maxConcurrentLoads": 4,
        "loadTimeout": 50000,
        "exportTimeout": 10000,
        "startTimeout": 25000
    },
    "runtime": {
        "updateInterval": 0
    },
    "security": {
        "whitelistedExtensions": [],
        "blacklistedExtensions": []
    },
    "logging": {
        "severity": "error",
        "printReport": false,
        "printLoadOrder": false,
        "printDependencyGraph": false,
        "printDigraphDot": false,
        "exportDigraphDot": ""
    }
}
